pipeline {
    agent any
    stages {
        stage('Clone Repository') {
            steps {
git branch: 'main', url: 'https://github.com/nandkishor12/code99.git'
sh 'kubectl cluster-info'
sh'kubectl get namespaces '
sh'kubectl get all'
sh'kubectl get all'
sh 'kubectl get cm '
sh 'kubectl get cm -n kube-system'
sh 'kubectl get cm my-config -o yaml'

            }
        }
    }

 }
    
