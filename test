pipeline {
    agent any
    stages {
        stage('Clone Repository') {
            steps {
                git branch: 'main', url: 'https://github.com/nandkishor12/code99.git'
            }
        }

        stage('Get Kubernetes Deployment') {
            steps {
                sh 'kubectl get deployments'
            }
        }
stage('Gkubectl cluster') {
            steps {
                sh 'kubectl cluster-info'
            }
        }
    }
}
