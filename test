pipeline {
    agent any
    stages {
        stage('Clone Repository') {
            steps {
                git branch: 'main', url: 'https://github.com/nandkishor12/code99.git'
            }
        }
    }
stages {
stage('Get Kubernetes services') {
            steps {
                sh 'kubectl get services'

            }
        }
    }
 }
    
